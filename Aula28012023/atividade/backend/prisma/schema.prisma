// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Chamado {
  id Int @id @default(autoincrement())
  numero         String @unique
  titulo         String
  descricao      String 
  categoria      String
  horasEstimadas Int
  prioridade     String
}